{
  "name": "convo-node",
  "version": "0.0.2",
  "description": "Convo node.js library",
  "keywords": [
    "convo"
  ],
  "main": "./lib/convo",
  "scripts": {
    "test": "node_modules/mocha/bin/mocha --timeout 5000 test/integration/*/*/*.js",
