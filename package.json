{
  "name": "convo-node",
  "version": "0.0.2",
  "description": "node.js library for Convo ChatOps tool",
  "keywords": [
    "convo"
  ],
  "main": "./lib/convo",
  "scripts": {
    "test": "node_modules/mocha/bin/mocha --timeout 5000 test/agents/*/*.js",
    "coverage": "node_modules/mocha/bin/mocha --require mocha-lcov-reporter --reporter mocha-lcov-reporter --timeout 5000 test/agents/*/*.js",
    "publish-coverage": "cat stage/coverage.lcov | node_modules/coveralls/bin/coveralls.js"
  },
  "dependencies": {
    "mustache": "^2.3.2",
    "sync": "^0.2.5",
    "underscore": "^1.9.1"
  },
  "devDependencies": {
    "coveralls": "^3.0.2",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0"
  },
  "engines": {
    "node": ">= 6.0.0"
  },
  "license": "MIT"
}
