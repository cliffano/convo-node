  },
  "dependencies": {
    "sync": "^0.2.5",
    "voca": "^1.4.0",
    "yaml": "^1.0.0-rc.7"
  },
  "devDependencies": {
    "coveralls": "^3.0.2",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0"
  },
  "engines": {
    "node": ">= 6.0.0"
  },
  "license": "MIT"
}
